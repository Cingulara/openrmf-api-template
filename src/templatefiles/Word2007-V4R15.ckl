				<STIG_DATA>
					<VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
					<ATTRIBUTE_DATA>SV-18564r1_rule</ATTRIBUTE_DATA>
				</STIG_DATA>
