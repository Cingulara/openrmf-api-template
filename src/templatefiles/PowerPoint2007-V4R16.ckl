					<ATTRIBUTE_DATA>DTOO290 - Make Hidden marks visible in PowerPoint</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
					<VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
					<ATTRIBUTE_DATA>SV-18943r1_rule</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
