					<ATTRIBUTE_DATA>DTOO104 - Disable user name and password</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
					<VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
					<ATTRIBUTE_DATA>SV-33407r1_rule</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
