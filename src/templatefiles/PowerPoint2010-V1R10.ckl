				</STIG_DATA>
				<STIG_DATA>
					<VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
					<ATTRIBUTE_DATA>SV-33600r1_rule</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
