					<ATTRIBUTE_DATA>DTBI320 - Security zone machine settings</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
					<VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
					<ATTRIBUTE_DATA>SV-45220r1_rule</ATTRIBUTE_DATA>
