				</STIG_DATA>
				<STIG_DATA>
					<VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
					<ATTRIBUTE_DATA>SV-18643r2_rule</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
