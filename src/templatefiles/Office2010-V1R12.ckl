					<ATTRIBUTE_DATA>DTOO191-ActiveX Control Initialization for Office</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
					<VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
					<ATTRIBUTE_DATA>SV-33453r1_rule</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
