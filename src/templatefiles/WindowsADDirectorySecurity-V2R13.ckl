				</STIG_DATA>
				<STIG_DATA>
					<VULN_ATTRIBUTE>Rule_ID</VULN_ATTRIBUTE>
					<ATTRIBUTE_DATA>SV-30996r3_rule</ATTRIBUTE_DATA>
				</STIG_DATA>
				<STIG_DATA>
